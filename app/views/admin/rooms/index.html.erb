<h1>Rooms#index</h1>
<p>Find me in app/views/rooms/index.html.erb</p>

<%= link_to "Create a New Room", new_admin_room_path %>

<% @rooms.each do |room| %>
  <h2><%= link_to room.name, admin_room_path(room) %></h2>
  <p>Amount of beds: <%= room.amount_of_beds %></p>
  <p>Price: <%= room.price %></p>
  <p>Hotel: <%= room.hotel.name %></p>
<% end%>