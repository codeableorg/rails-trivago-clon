<%= form_for [:admin, @hotel] do |f| %>
  <% if @hotel.errors.any? %>
    <div id="error_explanation">
      <ul>
        <% @hotel.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :start_date %>
    <%= f.text_field :start_date %>
  </div>

  <div class="field">
    <%= f.label :end_date %>
    <%= f.email_field :end_date %>
  </div>

  <div class="field">
    <%= f.label :paid_price %>
    <%= f.text_field :paid_price %>
  </div>

  <div class="field">
    <%= f.label :user_id %>
    <%= f.text_field :user_id %>
  </div>

  <div class="field">
    <%= f.label :room_id %>
    <%= f.text_field :room_id %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>