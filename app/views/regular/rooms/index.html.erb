<h1>Rooms#index</h1>
<%= form_tag(rooms_path, {:method => 'get'}) do %>
    <%= text_field_tag(:search, params[:search]) %>
    <%= submit_tag('Search', :name => nil) %>
<% end %>
<ul>
<% @rooms.each do |room| %>
  <li><%= link_to room.name, controller: "rooms", action: "show", id: room  %></li>
<% end %>
</ul>
